{
  "name": "client-ifoodapi",
  "version": "1.0.0",
  "description": "Uma **Client API** para o IFood que permite **gerenciar multiplas** sessões no IFood Marketplace API de forma simples, concedendo a possibilidade de obter informações de conta, **enviar pedidos**, ver pedidos já feitos.",
  "main": "./dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "npx tsc",
    "start": "NODE_ENV=production && npm run build && node ./dist/server.js",
    "dev": "NODE_ENV=dev && ts-node-dev --respawn ./src/server.ts",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leoelias023/client-ifoodapi.git"
  },
  "keywords": [
    "ifood",
    "api",
    "public",
    "free"
  ],
  "author": "Leonardo Elias de Oliveira <leoelias02@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leoelias023/client-ifoodapi/issues"
  },
  "homepage": "https://github.com/leoelias023/client-ifoodapi#readme",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.32",
    "@types/node-cron": "^2.0.3",
    "@types/pino": "^6.3.6",
    "husky": "^5.1.3",
    "pino-pretty": "^4.7.1",
    "prettier": "2.2.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">= v14.6.0"
  },
  "dependencies": {
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.13",
    "node-cron": "^3.0.0",
    "pino": "^6.11.2",
    "uuid": "^8.3.2"
  }
}
